{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "plugin:solid/recommended",
  "plugins": ["solid"],
  "overrides": [{
    "files": ["*.ts", "*.tsx"],
    "parser": "@typescript-eslint/parser"
  }],
  "rules": {
    "no-unused-vars": 1
  },
  "reportUnusedDisableDirectives": true
}
